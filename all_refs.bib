issn = {1029-8479},
journal = {Journal of High Energy Physics},
month = may,
number = {05},
pages = {067--067},
title = {{The AdS/QCD correspondence: still undelivered}},
url = {http://arxiv.org/abs/0811.3001 http://stacks.iop.org/1126-6708/2009/i=05/a=067?key=crossref.e7d746ea82ea0b5b19ec755f96482b5f},
volume = {2009},
