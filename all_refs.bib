month = apr,
number = {4},
pages = {843--882},
primaryClass = {hep-th},
title = {{Low Energy Hadron Physics in Holographic QCD}},
url = {http://arxiv.org/abs/hep-th/0412141},
volume = {113},
year = {2005}
}
